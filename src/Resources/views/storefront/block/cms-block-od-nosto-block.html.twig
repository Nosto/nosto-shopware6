{% block blocks_view_od_nosto_component_block %}
    <div class="od-nosto-block">
        {% set element = block.slots.getSlot('od_nosto') %}
        {% set columns = 1 %}
        <div class="col-12" data-cms-element-id="{{ element.id }}">
            {% block od_nosto_inner_block %}
                {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
            {% endblock %}
        </div>
    </div>
{% endblock %}
