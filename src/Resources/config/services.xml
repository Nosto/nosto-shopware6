<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Od\NostoIntegration\Model\Operation\FullCatalogSyncHandler">
            <tag name="od.job_handler" code="od-nosto-full-catalog-sync"/>
        </service>
        <service id="Od\NostoIntegration\Model\Operation\OrderSyncHandler">
            <argument type="service" id="order.repository"/>
            <tag name="od.job_handler" code="od-nosto-order-sync"/>
        </service>
    </services>
</container>