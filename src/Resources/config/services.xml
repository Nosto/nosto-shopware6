<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Od\NostoIntegration\Model\Operation\FullCatalogSyncHandler">
            <tag name="od.job_handler" code="od-nosto-full-catalog-sync"/>
        </service>

        <service id="Od\NostoIntegration\Service\ScheduledTask\OldJobCleanupScheduledTask">
            <tag name="shopware.scheduled.task"/>
        </service>

        <service id="Od\NostoIntegration\Service\ScheduledTask\OldJobCleanupScheduledTaskHandler">
            <argument type="service" id="scheduled_task.repository"/>
            <tag name="messenger.message_handler"/>
            <argument type="service" id="od_scheduler_job.repository"/>
        </service>
    </services>
</container>