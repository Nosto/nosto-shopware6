{% block nosto_job_listing_page %}
    <sw-page class="nosto-job-list">
        <template #smart-bar-actions>
            <sw-button
                    @click="onScheduleProductSync"
                    :isLoading="isLoading"
                    class="od-nosto-detail__smart-bar-edit-button"
                    variant="primary"
            >
                {{ $tc('nosto.listing.rescheduleActionLabel') }}
            </sw-button>
        </template>
        <template slot="content">
            <od-job-listing-index ref="odJobListing"
                               :autoReloadInterval="60000"
                               :jobTypes="nostoJobTypes">

            </od-job-listing-index>
        </template>
        <template #sidebar>
            <sw-sidebar>
                {% block sw_jobs_list_sidebar_refresh %}
                    <sw-sidebar-item
                            icon="default-arrow-360-left"
                            :title="$tc('sw-product.list.titleSidebarItemRefresh')"
                            @click="onRefresh"
                    />
                {% endblock %}
            </sw-sidebar>
        </template>
    </sw-page>
{% endblock %}